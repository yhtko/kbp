{
  "name": "kbp-sample",
  "private": true,
  "scripts": {
  "clean": "rimraf app config images dist",
  "build": "npm run clean && esbuild src/app/index.js --bundle --format=iife --outfile=app/index.js && cpx \"src/config/**\" config && cpx images/** images",
  "lint": "eslint .",
  "pack": "kintone-plugin-packer . --manifest manifest.json --out dist/kbp-sample.zip"
},
  "devDependencies": {
    "@kintone/plugin-packer": "^8.2.1",
    "cpx2": "^8.0.0",
    "esbuild": "^0.25.11",
    "eslint": "^9.38.0",
    "rimraf": "^6.0.1"
  }
}
